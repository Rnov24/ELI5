# Google API Key (Required)
# Replace with your actual Google API Key
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE

# Rate Limiting Settings (Optional, defaults provided in config.py)
# Maximum requests allowed per window per IP
RATE_LIMIT=100
# Time window in seconds (e.g., 3600 for 1 hour)
WINDOW=3600

# Redis Configuration (Required for rate limiting)
# Hostname for your Redis server
REDIS_HOST=localhost
# Port for your Redis server
REDIS_PORT=6379

# Optional Redis Settings (Uncomment and set if needed)
# REDIS_PASSWORD=your_redis_password
# REDIS_DB=0
